{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YourSuperSecretKeyHereThatShouldBeAtLeast32BytesLongForHS256",
    "Issuer": "AgentSystem",
    "Audience": "AgentSystemAPI",
    "DurationInMinutes": 60
  },
  "AgentSystem": {
    "SystemName": "AgentSystem",
    "SystemVersion": "1.0.0",
    "GlobalSettings": {
      "DefaultLanguage": "en-US",
      "EnableTelemetry": true,
      "MaxConcurrentRequests": 100
    }
  }
}